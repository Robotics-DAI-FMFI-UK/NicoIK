    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": " -g -c -i -v -a -o 0 0 1.3 -io 0 0 1.57",
            "console": "integratedTerminal",
            "justMyCode": False
        }
    ]
}